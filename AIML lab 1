        "from queue import Queue\n",
        "graph = {0:[1,2,3],1:[0,2],2:[0,1,3,5],3:[0,2,4],4:[3,5,7],5:[4,6,7],6:[5,7],7:[4,5,6],8:[4,6,7]}\n",
        "print(\"the adjacency List representing the graph is:\")\n",
        "print(graph)"
